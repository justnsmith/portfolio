{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@data/*": [
                "src/data/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@hooks": [
                "src/hooks/index.ts"
            ],
            "types/*": [
                "src/types/*"
            ],
            "@types": [
                "src/types/index.ts"
            ],
            "@ui/*": [
                "src/components/ui/*"
            ]
        }
    }
}
